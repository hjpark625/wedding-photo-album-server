{
  "family": "wedding-photo-task",
  "executionRoleArn": "arn:aws:iam::680738090849:role/wedding-photo-ecs-role",
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "wedding-photo-container",
      "image": "",
      "environment": [
        {
          "name": "PORT",
          "value": 4000
        },
        {
          "name": "CSRF_SECRET_KEY",
          "value": ""
        },
        {
          "name": "AWS_ACCESS_KEY_ID",
          "value": ""
        },
        {
          "name": "AWS_SECRET_ACCESS_KEY",
          "value": ""
        },
        {
          "name": "AWS_REGION",
          "value": ""
        },
        {
          "name": "AWS_S3_BUCKET_NAME",
          "value": ""
        },
        {
          "name": "AWS_CLOUDFRONT_DOMAIN",
          "value": ""
        },
        {
          "name": "CORS_ORIGIN",
          "value": ""
        },
        {
          "name": "MAIN_DOMAIN",
          "value": ""
        }
      ],
      "essential": true,
      "portMappings": [
        {
          "containerPort": 4000,
          "hostPort": 4000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/wedding-photo-server",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
